{
  "name": "docfinder",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "src/server.js",
  "bin": {
    "docfinder": "src/cli.js"
  },
  "scripts": {
    "start": "node src/server.js",
    "init": "node scripts/init-tokens.js",
    "dev": "nodemon src/server.js",
  "test": "NODE_ENV=test mocha 'test/**/*.spec.js' --timeout 10000 --exit",
  "test:watch": "NODE_ENV=test mocha 'test/**/*.spec.js' --timeout 10000 --watch --exit",
  "test:coverage": "NODE_ENV=test nyc --reporter=html --reporter=text mocha 'test/**/*.spec.js' --timeout 10000 --exit"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@azure/msal-node": "^2.6.4",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "fast-glob": "^3.3.2",
    "fuse.js": "^7.0.0",
    "google-auth-library": "^8.0.0",
    "googleapis": "^133.0.0",
    "js-yaml": "^4.1.1",
    "keytar": "^7.9.0",
    "node-fetch": "^3.3.2",
    "open": "^10.1.0",
    "winston": "^3.13.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "mocha": "^10.8.2",
    "mochawesome": "^7.1.3",
    "nyc": "^15.1.0",
    "sinon": "^18.0.1",
    "supertest": "^6.3.3"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/*.spec.js",
      "test/**/*.js"
    ]
  }
}
